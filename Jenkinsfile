pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build stage') {
            steps {
                echo 'building'
            }
        }
         stage('Deploy Stage') {
            steps {
                echo 'Deploying'
            }
        }
         stage('Test Stage') {
            steps {
                echo 'Testing'
            }
        }
        stage('Release Stage') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
